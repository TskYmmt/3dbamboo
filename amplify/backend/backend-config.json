{
  "api": {
    "tanabataAPI": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "tanzakuAPI",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "function": {
    "tanzakuAPI": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "tanzakuS3",
          "attributes": [
            "BucketName"
          ]
        }
      ]
    }
  },
  "storage": {
    "tanzakuS3": {
      "service": "S3",
      "providerPlugin": "awscloudformation"
    }
  }
}